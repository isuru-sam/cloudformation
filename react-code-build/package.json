{
  "name": "website",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "dotenv": "^6.2.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-scripts": "^5.0.1"
  },
  "scripts": {
    "start:dev": "sh -ac '. ./.env.development; react-scripts start';",
    "build:dev": "sh -ac '. ./.env.development; react-scripts build';",
    "build:stage": "sh -ac '. ./.env.staging; react-scripts build';",
    "build:prod": "sh -ac '. ./.env.production; react-scripts build';",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
